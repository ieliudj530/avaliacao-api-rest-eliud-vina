import java.util.ArrayList;
import java.util.List;     
import java.util.Objects; 

public class Aluno {
    private int id;
    private String nome;
    private int idade;
    private String email;
    private String curso;

    public Aluno(int id, String nome, int idade, String email, String curso) {
        this.id = id;
        this.nome = nome;
        this.idade = idade;
        this.email = email;
        this.curso = curso;
    }

    public int getId() {
        return id;
    }
    public String getNome() {
        return nome;
    }

    public String getEmail() {
        return email;
    }

    public String getCurso() {
        return curso;
    }

    public int getIdade() {
        return idade;
    }
}